@model NetDuino.Models.ApplicationUser

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@{
    if (Model.Arduinos.Count == 0)
    {
        Html.ActionLink("Click here to add new device.", "Create");
    }
    else
    {
        foreach (var item in Model.Arduinos)
        {
            <div>
                <h2>@item.Name</h2>
            </div>
        }
    }
}


